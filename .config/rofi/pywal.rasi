* {
	background-color: #14161833;
	text-color: #c4c4c5;
	spacing: 0;
	width: 720px;
}

inputbar {
	border: 0 0 1px 0;
	children: [prompt, entry];
}

prompt {
	padding: 16px;
}

textbox {
	background-color: #141618;
	border-color: #141618;
	padding: 8px 16px;
}

entry {
	padding: 16px;
}

listview {
	cycle: false;
	margin: 0 0 -1px 0;
	scrollbar: false;
}

element {
	padding: 16px;
}

element selected {
	background-color: #2b2b34;
	text-color: #c4c4c5;
}