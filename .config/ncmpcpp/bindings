def_key "/"
	find_item_forward
def_key "n"
	next_found_item
def_key "N"
	previous_found_item

def_key "-"
	volume_down
def_key "="
	volume_up

def_key "j"
	scroll_down
def_key "k"
	scroll_up
def_key "ctrl-u"
	page_up
def_key "ctrl-d"
	page_down
def_key "g"
	move_home
def_key "G"
	move_end

def_key "b"
	jump_to_browser
def_key "."
	show_lyrics

def_key "h"
	previous_column
def_key "l"
	next_column

def_key "m"
	move_selected_items_up
def_key "M"
  move_selected_items_down

def_key "v"
	select_item
	scroll_down
def_key "V"
	reverse_selection
def_key "ctrl-v"
	remove_selection
